{
    "name": "project-environment",
    "version": "0.0.1",
    "private": true,
    "license": "MIT",
    "engines": {
        "node": ">=18.17.0 <19.0.0"
    },
    "scripts": {
        "code:analyse": "eslint --ext .js --ext .vue --ext .yaml --ext .yml --ext .ts --ext .json --ignore-path .eslintignore .",
        "code:analyse:fix": "eslint --ext .js --ext .vue --ext .yaml --ext .yml --ext .ts --ext .json --ignore-path .eslintignore . --fix",
        "code:style": "yarn prettier --ignore-path=.eslintignore --list-different \"*.{js,ts,tsx,json,css,vue,md,yml,yaml}\"",
        "code:style:fix": "yarn prettier --ignore-path=.eslintignore --write \"*.{js,ts,tsx,json,css,vue,md,yml,yaml}\"",
        "project:info": "echo '⚠️  \"environment\"'"
    },
    "devDependencies": {
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-markdown": "^3.0.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-yaml": "^0.5.0",
        "prettier": "^3.0.2"
    },
    "dependencies": {
        "colors": "^1.4.0",
        "commander": "^10.0.1",
        "conf": "^11.0.1",
        "cryptr": "^6.2.0",
        "dotenv": "^16.1.1",
        "fast-glob": "^3.2.12",
        "fs": "^0.0.1-security",
        "js-base64": "^3.7.5",
        "lodash.get": "^4.4.2",
        "lodash.set": "^4.3.2",
        "yaml": "^2.3.2"
    }
}
