---
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts'
import DefaultLayout from '../layouts/DefaultLayout.astro'
import Placeholder from '../components/Placeholder.astro'
---

<DefaultLayout siteTitle=`${SITE_TITLE}` siteDescription=`${SITE_DESCRIPTION}`>
    <Placeholder name="Hero" minHeight={500}></Placeholder>
    <Placeholder name="ContentListing" description="Lists collected content" minHeight={1200}></Placeholder>
</DefaultLayout>
